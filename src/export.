static char	*remalloc_env(char **env)
{
	
}

static void	put_var_env(char ***env, char *arg)
{
	
}

void	our_export(t_cmd *cmd, char **env)
{
	int	i;

	i = 0;
	while (cmd->cmd[++i])
	{
		if (ft_strchr(cmd->cmd[i], '='))
			put_var(&env, cmd->cmd[i]);
	}
}

